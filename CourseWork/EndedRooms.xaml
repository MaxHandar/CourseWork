<Window x:Class="CourseWork.EndedRooms"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseWork"
        mc:Ignorable="d"
        Title="CourseWork" Height="600" Width="600">
    <Grid Background="#9D9D9D">
        <Border MinWidth="400" Margin="15" Background="White" VerticalAlignment="Center" Padding="90" MaxWidth="500" CornerRadius="20">
            <Border.Effect>
                <DropShadowEffect BlurRadius="30" Color="DarkGray" ShadowDepth="0"></DropShadowEffect>
            </Border.Effect>
            <StackPanel>
                <TextBlock Text="Список завершенных комнат" FontSize="20" FontWeight="Bold" Margin="25 0 0 100"></TextBlock>
                <Grid Margin="0 40 0 0">
                    <Button HorizontalAlignment="Center" Content="Вернуться назад"  FontSize="20" Click="BackToMain_Click"></Button>
                </Grid>
            </StackPanel>
        </Border>
    </Grid>
</Window>